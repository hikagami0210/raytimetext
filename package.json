{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raytimetext",
  "title": "RayTimeText",
  "description": "Provides automatic text input that changes based on the time.",
  "icon": "clock.png",
  "author": "hikagami0210",
  "owner": "hikagami",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "RayTimeText",
      "description": "Provides automatic text input that changes based on the time.",
      "mode": "no-view"
    },
    {
      "name": "create",
      "title": "Create RayTimeText",
      "description": "Create time ranges and messages",
      "mode": "view"
    },
    {
      "name": "list",
      "title": "List RayTimeText",
      "description": "Display time ranges and messages",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.75.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
